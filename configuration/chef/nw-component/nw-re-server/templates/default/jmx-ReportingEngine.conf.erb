                <Stat>
                    <Meta>
                      category "HealthStatistics"
                    </Meta>
                </Stat>
            </DataHandler>
        </Value>
        <Value>
            Type "gauge"
            Table true
            InstancePrefix "REHealthStats-"
            Attribute "REHealthStats"
            <DataHandler "com.rsa.netwitness.sms.jmx.SMSJmxDataHandler">
                <Stat "ScheduledTasksPool-AverageExecutionTime">
                    <Meta>
                      name "Schedule Task Pool Average Execution Time"
                      category "HealthStatistics"
                      description "Schedule Task Pool Average Execution Time"
                    </Meta>
                </Stat>
                 <Stat "ScheduledTasksPool-Queued">
                    <Meta>
                      name "Schedule Task Pool Queued"
                      category "HealthStatistics"
                      description "Number of pending tasks waiting in queue to be executed"
                    </Meta>
                </Stat>
                <Stat "ScheduledTasksPool-Running">
                    <Meta>
                      name "Schedule Task Pool Running"
                      category "HealthStatistics"
                      description "Number of running RE tasks such as formatting tasks and output actions"
                    </Meta>
                </Stat>
                <Stat "ScheduledTasksPool-Utilization">
                    <Meta>
                      name "Schedule Task Pool Utilization"
                      category "HealthStatistics"
                      description "Score (0-10), indicator of how busy RE is in executing tasks like formatting, output actions and other tasks"
                    </Meta>
                </Stat>

                #SharedTasksPool
                <Stat "SharedTasksPool-AverageExecutionTime">
                    <Meta>
                      name "Shared Task Pool Average Execution Time"
                      category "HealthStatistics"
                      description "Shared Task Pool Average Execution Time"
                    </Meta>
                </Stat>
                 <Stat "SharedTasksPool-Queued">
                    <Meta>
                      name "Shared Task Pool Queued"
                      category "HealthStatistics"
                      description "Number of pending tasks waiting in queue to be executed"
                    </Meta>
                </Stat>
                <Stat "SharedTasksPool-Running">
                    <Meta>
                      name "Shared Task Pool Running"
                      category "HealthStatistics"
                      description "Number of running scheduled tasks like clean-up of expired results"
                    </Meta>
                </Stat>
                <Stat "SharedTasksPool-Utilization">
                    <Meta>
                      name "Shared Task Pool Utilization"
                      category "HealthStatistics"
                      description "Score (0-10), indicator of how busy RE is in executing scheduled tasks, relative to the capacity and number of waiting tasks"
                    </Meta>
                </Stat>
            </DataHandler>
        </Value>
    </MBean>

    #####################
    # Connection block  #
    #####################
    <Connection>
      Host "<%= @node_uuid %>"
      ServiceURL "service:jmx:rmi:///jndi/rmi://localhost:50011/carlos"
      # NAMING CONVENTION = InstancePrefix "<componentId>_jmx-"
      InstancePrefix "reportingengine_jmx-"
      Collect "GarbageCollector"
      Collect "Memory"
      Collect "OperatingSystem"
      Collect "RERequestStatistic"
      Collect "DiskSpaceStats"
      Collect "ZipResultStoreStats"
      Collect "AlertStatistics"
      Collect "ChartStatusHandler"
      Collect "QueryExecutionStatistics"
      Collect "ReportStatistics"
      Collect "HealthStatistics"
    </Connection>
  </Plugin>
</Plugin>
