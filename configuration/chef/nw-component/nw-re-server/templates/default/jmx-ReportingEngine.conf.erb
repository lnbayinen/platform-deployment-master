    <MBean "HealthStatistics">
        ObjectName "com.rsa.soc.re:id=healthStatistics,subType=HealthStatistics,type=Metrics"
        InstancePrefix "HealthStatistics"
        <Value>
            Type "gauge"
            Table true
            Attribute "DataSourceHealthStats"
