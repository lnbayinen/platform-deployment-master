<Plugin generic_cpp>
    PluginModulePath "/usr/lib64/collectd"

    <Module "NgNativeReader" "NwLogCollector-FastUpdate">
        port      "56001"
        ssl       "yes"
        keypath   "<%= @keyfile %>"
        certpath  "<%= @certfile %>"
        interval  "10"

        <stats>
            <stat>
                path             "/sys/stats/service.status"
                plugin           "logcollector_sys"
                plugin_instance  ""
                type             "string"
                type_instance    "service.status"
                <meta>
                    name         "Service State"
                    description  "%description%"
                    category     "ProcessInfo"
                    path         "%path%"
                </meta>
            </stat>
            <stat>
                path             "/logcollection/<.*>/stats/state"
                plugin           "logcollector_logcollection.%1%"
                plugin_instance  ""
                type             "enumerated"
                type_instance    "state"
                <meta>
                    name         "&TITLE_CASE(%name%)"
                    description  "%description%"
                    category     "&TITLE_CASE(%1%) Collection"
                    path         "%path%"
                    p1           "%1%"
                </meta>
                <enumData>
                    stopped    "0"
                    started    "1"
                    stopping   "2"
                    starting   "3"
                    paused     "4"
                </enumData>
            </stat>
            <stat>
                path             "/logcollection/<.*>/stats/eventsources/total_events_rate"
                plugin           "logcollector_logcollection.%1%"
                plugin_instance  ""
                type             "gauge"
                type_instance    "total_events_rate"
                <meta>
                    name         "&TITLE_CASE(%name%)"
                    description  "%description%"
                    category     "&TITLE_CASE(%1%) Collection"
                    subitem      "All"
                    path         "%path%"
                    p1           "%1%"
                    units         "eps"
                </meta>
            </stat>
            <stat>
                path             "/logcollection/<.*>/stats/eventsources/<^(?!total_.*).*$>/total_events_rate"
                plugin           "logcollector_logcollection.%1%"
                plugin_instance  "%2%"
                type             "gauge"
