<Plugin generic_cpp>
    PluginModulePath "/usr/lib64/collectd"

    <Module "NgNativeReader" "NwConcentrator-FastUpdate">
        port      "56005"
        ssl       "yes"
        keypath   "<%= @keyfile %>"
        certpath  "<%= @certfile %>"
        interval  "10"

        <stats>
            <stat>
                path             "/sys/stats/service.status"
                plugin           "concentrator_sys"
                plugin_instance  ""
                type             "string"
                type_instance    "service.status"
                <meta>
                    name         "Service State"
                    description  "%description%"
                    category     "ProcessInfo"
                    path         "%path%"
                </meta>
            </stat>
            <stat>
                path             "/concentrator/stats/status"
                plugin           "concentrator_concentrator"
                plugin_instance  ""
                type             "string"
                type_instance    "status"
                <meta>
                    name         "%name%"
                    description  "%description%"
                    category     "Concentrator"
                    path         "%path%"
                </meta>
            </stat>
            <stat>
                path             "/concentrator/stats/meta.rate"
                plugin           "concentrator_concentrator"
                plugin_instance  ""
                type             "gauge"
                type_instance    "meta.rate"
                <meta>
                    name         "%name%"
                    description  "%description%"
                    category     "Concentrator"
                    path         "%path%"
                </meta>
            </stat>
            <stat>
                path             "/concentrator/stats/session.rate"
                plugin           "concentrator_concentrator"
                plugin_instance  ""
                type             "gauge"
                type_instance    "session.rate"
                <meta>
                    name         "%name%"
                    description  "%description%"
                    category     "Concentrator"
                    path         "%path%"
                </meta>
            </stat>
            <stat>
                path             "/sdk/stats/queries.active"
                plugin           "concentrator_sdk"
                plugin_instance  ""
                type             "gauge"
                type_instance    "queries.active"
                <meta>
                    name         "%name%"
                    description  "%description%"
                    category     "Concentrator"
                    path         "%path%"
                </meta>
            </stat>
            <stat>
                path             "/sdk/stats/queries.pending"
                plugin           "concentrator_sdk"
                plugin_instance  ""
                type             "gauge"
                type_instance    "queries.pending"
                <meta>
                    name         "%name%"
                    description  "%description%"
                    category     "Concentrator"
                    path         "%path%"
                </meta>
            </stat>
        </stats>
    </Module>

